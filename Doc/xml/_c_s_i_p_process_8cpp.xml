<sp/><sp/><sp/><sp/><sp/><sp/><sp/><sp/>if(!WriteFile(m_hLog,<sp/>(LPCVOID)chMsg.data(),<sp/>wcslen(chMsg.data())<sp/>*<sp/>sizeof(wchar_t),<sp/>&amp;dwBytesWritten,<sp/>NULL))<sp/>AddToMessageLog(LPTSTR(chMsg.data()));
<sp/><sp/><sp/><sp/>}
}

